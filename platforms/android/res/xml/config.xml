<?xml version='1.0' encoding='utf-8'?>
<widget id="net.edrake.adviceowl" version="1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <preference name="loglevel" value="INFO" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="load-url-timeout" value="60000" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <author email="drakee@gmail.com" href="https://github.com/psydrake">
        Drake Emko
    </author>
    <content src="index.html" />
</widget>
